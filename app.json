{
  "expo": {
    "name": "Basketball Coach",
    "slug": "basketball-coach",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo2.png",
    "splash": {
      "image": "./assets/load4.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",{
          "photosPermission": "Você precisa permitir acesso à sua câmera para usar esta aplicação"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.willians.oneplayer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
